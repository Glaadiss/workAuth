                    // funkcja wychwytujÄ…ca dane twarzy

                    function getWsp () {
                    var x = window.rectX;
                    var y = window.rectY;
                    var width = window.rectW;
                    var height = window.rectH;
                    var eyeX = window.eyeX - x;
                    var eyeY = window.eyeY - y;
                    var eyeX2 = window.eyeX2 - x;
                    var eyeY2 = window.eyeY2 - y;
                    var noseX = window.noseX - x;
                    var noseY = window.noseY - y;
                    var noseX2 = window.noseX2 - x;
                    var noseY2 = window.noseY2 - y;
                    var midX = window.midX - x;
                    var midY = window.midY - y;
                    var eyeToEye = lineLength(eyeX, eyeY, eyeX2, eyeY2);
                    var noseWidth = lineLength(noseX,noseY,noseX2, noseY2);
                    var wsp1 = eyeToEye/width;
                    var wsp2 = noseWidth/width;
                   return {
                        width: window.rectW,
                        height: window.rectH,
                        eyeX: window.eyeX,
                        eyeY: window.eyeY,
                        eyeX2: window.eyeX2,
                        eyeY2: window.eyeY2,
                        noseX: window.noseX,
                        noseY: window.noseY,
                        noseX2: window.noseX2,
                        noseY2: window.noseX2,
                        wsp1: wsp1,
                        wsp2: wsp2
                   }
                }

    </script>

